                <operation>equals</operation>
                <value>OR</value>
            </criteriaItems>
            <criteriaItems>
                <field>Campaign.Name</field>
                <operation>equals</operation>
                <value>Frontier.com/Enterprise Leads</value>
            </criteriaItems>
            <criteriaItems>
                <field>Lead.LeadSource</field>
                <operation>equals</operation>
                <value>Assisted Living - Email</value>
            </criteriaItems>
            <criteriaItems>
                <field>Lead.LeadSource</field>
                <operation>equals</operation>
                <value>Think Big (Website)</value>
            </criteriaItems>
            <criteriaItems>
                <field>Lead.Web2Lead__c</field>
