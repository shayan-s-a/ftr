    <assignmentRule>
        <fullName>Frontier%2Ecom Lead Routing</fullName>
        <active>false</active>
        <ruleEntry>
            <assignedTo>clearlink_lead_testing</assignedTo>
            <assignedToType>Queue</assignedToType>
            <booleanFilter>1 OR 2</booleanFilter>
