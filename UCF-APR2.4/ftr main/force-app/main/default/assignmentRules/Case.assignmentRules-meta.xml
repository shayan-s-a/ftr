            <criteriaItems>
                <field>Case.RecordTypeId</field>
                <operation>equals</operation>
                <value>Account Activity</value>
            </criteriaItems>
            <criteriaItems>
                <field>Case.Case_Reason_Other__c</field>
                <operation>equals</operation>
                <value>COBRA Billing Portal</value>
            </criteriaItems>
            <criteriaItems>
                <field>Case.RecordTypeId</field>
                <operation>equals</operation>
                <value>Training Request</value>
            </criteriaItems>
            <criteriaItems>
                <field>Case.Training_Category__c</field>
                <operation>equals</operation>
                <value>COBRA Billing Portal</value>
            </criteriaItems>
            <criteriaItems>
                <field>Case.Case_Reason_Other__c</field>
