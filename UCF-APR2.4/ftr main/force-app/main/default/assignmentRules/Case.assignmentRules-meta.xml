                <operation>equals</operation>
                <value>Billing Issue</value>
            </criteriaItems>
        </ruleEntry>
        <ruleEntry>
            <formula>OR( (RecordType.Name = &apos;Salesforce Admin&apos;), (RecordType.Name = &apos;Sales Enablement Team&apos; &amp;&amp; Billing_Support_Needed__c &amp;&amp; RVP__c = &apos;RVP BAM&apos; &amp;&amp; NOT(CONTAINS(Subject, &apos;Churn&apos;))), (RecordType.Name = &apos;Sales Enablement Team&apos; &amp;&amp; NOT(Billing_Support_Needed__c) &amp;&amp; RVP__c = &apos;RVP West&apos; &amp;&amp; NOT(CONTAINS(Subject, &apos;Churn&apos;))), (RecordType.Name = &apos;Sales Enablement Team&apos; &amp;&amp; NOT(Billing_Support_Needed__c) &amp;&amp; RVP__c = &apos;RVP Central&apos; &amp;&amp; NOT(CONTAINS(Subject, &apos;Churn&apos;))), (RecordType.Name = &apos;Sales Enablement Team&apos; &amp;&amp; NOT(Billing_Support_Needed__c) &amp;&amp; RVP__c = &apos;RVP East&apos; &amp;&amp; NOT(CONTAINS(Subject, &apos;Churn&apos;))), (RecordType.Name = &apos;Sales Enablement Team&apos; &amp;&amp; NOT(Billing_Support_Needed__c) &amp;&amp; RVP__c = &apos;RVP BAM&apos; &amp;&amp; NOT(CONTAINS(Subject, &apos;Churn&apos;))), (RecordType.Name = &apos;Sales Enablement Team&apos; &amp;&amp; CONTAINS(Subject, &apos;Churn Alert&apos;)), (RecordType.Name = &apos;Sales Enablement Team&apos; &amp;&amp; RVP__c = &apos;RVP Federal&apos;) )</formula>
