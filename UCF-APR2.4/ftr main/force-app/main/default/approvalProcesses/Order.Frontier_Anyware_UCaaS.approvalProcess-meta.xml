<?xml version="1.0" encoding="UTF-8"?>
<ApprovalProcess xmlns="http://soap.sforce.com/2006/04/metadata">
    <active>true</active>
    <allowRecall>false</allowRecall>
    <allowedSubmitters>
        <type>owner</type>
    </allowedSubmitters>
    <approvalPageFields>
        <field>OrderNumber</field>
        <field>Owner</field>
    </approvalPageFields>
    <approvalStep>
        <allowDelegate>false</allowDelegate>
        <approvalActions>
            <action>
                <name>Null_Dynamic_Queue_Assignment</name>
                <type>FieldUpdate</type>
            </action>
            <action>
                <name>Stage_to_In_Progress</name>
                <type>FieldUpdate</type>
            </action>
            <action>
                <name>Step_2_Approval</name>
                <type>FieldUpdate</type>
            </action>
        </approvalActions>
        <assignedApprover>
            <approver>
                <name>CPE_DCS</name>
                <type>queue</type>
            </approver>
            <whenMultipleApprovers>Unanimous</whenMultipleApprovers>
        </assignedApprover>
        <entryCriteria>
            <formula>ISBLANK(CPE_DCS__c)</formula>
        </entryCriteria>
        <ifCriteriaNotMet>GotoNextStep</ifCriteriaNotMet>
        <label>Step 1</label>
        <name>Step_1</name>
    </approvalStep>
    <approvalStep>
        <allowDelegate>false</allowDelegate>
        <approvalActions>
            <action>
                <name>Step_3_Approval</name>
                <type>FieldUpdate</type>
            </action>
            <action>
                <name>UCaaS_Dyanmic_Queue_Assignment_Step2</name>
                <type>FieldUpdate</type>
            </action>
        </approvalActions>
        <assignedApprover>
            <approver>
                <name>Hot_Cuts_Frontier_Anyware</name>
                <type>queue</type>
            </approver>
            <approver>
                <name>VoIP_Translations</name>
                <type>queue</type>
            </approver>
            <approver>
                <name>CPE_DCS__c</name>
                <type>relatedUserField</type>
            </approver>
            <whenMultipleApprovers>Unanimous</whenMultipleApprovers>
        </assignedApprover>
        <entryCriteria>
            <booleanFilter>1 OR 2 OR 3</booleanFilter>
            <criteriaItems>
                <field>Order.Hot_Cut__c</field>
                <operation>equals</operation>
            </criteriaItems>
            <criteriaItems>
                <field>Order.VoIP_Translations__c</field>
                <operation>equals</operation>
            </criteriaItems>
            <criteriaItems>
                <field>Order.CPE_DCS_Stage__c</field>
                <operation>equals</operation>
                <value>Review Order,Schedule CDC Call</value>
            </criteriaItems>
        </entryCriteria>
        <ifCriteriaNotMet>GotoNextStep</ifCriteriaNotMet>
        <label>Step 2</label>
        <name>Step_2</name>
        <rejectBehavior>
            <type>RejectRequest</type>
        </rejectBehavior>
    </approvalStep>
    <approvalStep>
        <allowDelegate>false</allowDelegate>
        <approvalActions>
            <action>
                <name>Order_Complete_Approval</name>
                <type>FieldUpdate</type>
            </action>
        </approvalActions>
        <assignedApprover>
            <approver>
                <name>CPECOE_Dev</name>
                <type>queue</type>
            </approver>
            <approver>
                <name>NetworkCOE_Dev</name>
                <type>queue</type>
            </approver>
            <approver>
                <name>Hot_Cut__c</name>
                <type>relatedUserField</type>
            </approver>
            <whenMultipleApprovers>Unanimous</whenMultipleApprovers>
        </assignedApprover>
        <entryCriteria>
            <booleanFilter>1 OR 2 OR 3</booleanFilter>
            <criteriaItems>
                <field>Order.Broadband_COE__c</field>
                <operation>equals</operation>
            </criteriaItems>
            <criteriaItems>
                <field>Order.CPE_COE__c</field>
                <operation>equals</operation>
            </criteriaItems>
            <criteriaItems>
                <field>Order.Hot_Cut_Stage__c</field>
                <operation>equals</operation>
                <value>Review Order</value>
            </criteriaItems>
        </entryCriteria>
        <ifCriteriaNotMet>GotoNextStep</ifCriteriaNotMet>
        <label>Step 3</label>
        <name>Step_3</name>
        <rejectBehavior>
            <type>RejectRequest</type>
        </rejectBehavior>
        <rejectionActions>
            <action>
                <name>Null_Dynamic_Queue_Assignment</name>
                <type>FieldUpdate</type>
            </action>
        </rejectionActions>
    </approvalStep>
    <approvalStep>
        <allowDelegate>false</allowDelegate>
        <approvalActions>
            <action>
                <name>CPE_COE</name>
                <type>FieldUpdate</type>
            </action>
            <action>
                <name>DCS_Stage_Complete</name>
                <type>FieldUpdate</type>
            </action>
            <action>
                <name>Hot_Cuts_Completed</name>
                <type>FieldUpdate</type>
            </action>
            <action>
                <name>Network_COE_Completed</name>
                <type>FieldUpdate</type>
            </action>
            <action>
                <name>Translations_Completed</name>
                <type>FieldUpdate</type>
            </action>
        </approvalActions>
        <assignedApprover>
            <approver>
                <name>Hot_Cut__c</name>
                <type>relatedUserField</type>
            </approver>
            <approver>
                <name>Broadband_COE__c</name>
                <type>relatedUserField</type>
            </approver>
            <approver>
                <name>VoIP_Translations__c</name>
                <type>relatedUserField</type>
            </approver>
            <approver>
                <name>CPE_DCS__c</name>
                <type>relatedUserField</type>
            </approver>
            <approver>
                <name>CPE_COE__c</name>
                <type>relatedUserField</type>
            </approver>
            <whenMultipleApprovers>Unanimous</whenMultipleApprovers>
        </assignedApprover>
        <label>Order Complete</label>
        <name>Order_Complete</name>
        <rejectBehavior>
            <type>RejectRequest</type>
        </rejectBehavior>
    </approvalStep>
    <emailTemplate>unfiled$public/FAW_UcaaS_Order_Notification</emailTemplate>
    <enableMobileDeviceAccess>true</enableMobileDeviceAccess>
    <entryCriteria>
        <criteriaItems>
            <field>Order.Product__c</field>
            <operation>equals</operation>
            <value>Frontier AnyWare UCaaS - BYOA,Frontier AnyWare UCaaS + Ethernet,Frontier AnyWare UCaaS + Broadband</value>
        </criteriaItems>
        <criteriaItems>
            <field>Order.RecordType</field>
            <operation>equals</operation>
            <value>Change Order,Move Order,New Order,Upgrade/Downgrade Order</value>
        </criteriaItems>
        <criteriaItems>
            <field>Order.UcaaS_Cutover_Flag__c</field>
            <operation>equals</operation>
            <value>True</value>
        </criteriaItems>
        <criteriaItems>
            <field>Order.Location_Info_Exists__c</field>
            <operation>greaterThan</operation>
            <value>0</value>
        </criteriaItems>
    </entryCriteria>
    <finalApprovalActions>
        <action>
            <name>Stage_to_Complete</name>
            <type>FieldUpdate</type>
        </action>
        <action>
            <name>UCaaS_Approval_Process_Complete</name>
            <type>FieldUpdate</type>
        </action>
    </finalApprovalActions>
    <finalApprovalRecordLock>true</finalApprovalRecordLock>
    <finalRejectionActions>
        <action>
            <name>Reject_to_AE</name>
            <type>FieldUpdate</type>
        </action>
    </finalRejectionActions>
    <finalRejectionRecordLock>false</finalRejectionRecordLock>
    <initialSubmissionActions>
        <action>
            <name>Lock_Order</name>
            <type>FieldUpdate</type>
        </action>
        <action>
            <name>Stage_to_Assignment</name>
            <type>FieldUpdate</type>
        </action>
        <action>
            <name>Step_1_Approval</name>
            <type>FieldUpdate</type>
        </action>
        <action>
            <name>UCaaS_Dyanmic_Queue_Assignment</name>
            <type>FieldUpdate</type>
        </action>
    </initialSubmissionActions>
    <label>Frontier Anyware -UCaaS</label>
    <processOrder>19</processOrder>
    <recallActions>
        <action>
            <name>Reject_to_AE</name>
            <type>FieldUpdate</type>
        </action>
    </recallActions>
    <recordEditability>AdminOrCurrentApprover</recordEditability>
    <showApprovalHistory>false</showApprovalHistory>
</ApprovalProcess>
