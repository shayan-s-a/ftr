<?xml version="1.0" encoding="UTF-8"?>
<ApprovalProcess xmlns="http://soap.sforce.com/2006/04/metadata">
    <active>true</active>
    <allowRecall>true</allowRecall>
    <allowedSubmitters>
        <type>owner</type>
    </allowedSubmitters>
    <approvalPageFields>
        <field>OrderNumber</field>
        <field>Name</field>
        <field>Owner</field>
        <field>Order_Stage__c</field>
        <field>Order_Created_Date__c</field>
        <field>Reject_Reason__c</field>
    </approvalPageFields>
    <approvalStep>
        <allowDelegate>false</allowDelegate>
        <approvalActions>
            <action>
                <name>Approve_to_CPM_QA_Review</name>
                <type>FieldUpdate</type>
            </action>
            <action>
                <name>Owner_to_CPM_Queue</name>
                <type>FieldUpdate</type>
            </action>
        </approvalActions>
        <assignedApprover>
            <approver>
                <name>Sales_Engineering</name>
                <type>queue</type>
            </approver>
            <whenMultipleApprovers>FirstResponse</whenMultipleApprovers>
        </assignedApprover>
        <description>Move to CPM QA</description>
        <label>Sales Engineering</label>
        <name>Sales_Engineering</name>
        <rejectionActions>
            <action>
                <name>Reject_to_AE</name>
                <type>FieldUpdate</type>
            </action>
        </rejectionActions>
    </approvalStep>
    <approvalStep>
        <allowDelegate>true</allowDelegate>
        <approvalActions>
            <action>
                <name>Owner_to_CNP</name>
                <type>FieldUpdate</type>
            </action>
            <action>
                <name>Stage_to_CNP_Quality_Assurance</name>
                <type>FieldUpdate</type>
            </action>
        </approvalActions>
        <assignedApprover>
            <approver>
                <name>CPM</name>
                <type>queue</type>
            </approver>
            <whenMultipleApprovers>FirstResponse</whenMultipleApprovers>
        </assignedApprover>
        <description>Move to CNP Quality Assurance</description>
        <label>CPM QA</label>
        <name>CPM_QA</name>
        <rejectBehavior>
            <type>RejectRequest</type>
        </rejectBehavior>
        <rejectionActions>
            <action>
                <name>Order_Rejected</name>
                <type>Alert</type>
            </action>
            <action>
                <name>Reject_to_AE</name>
                <type>FieldUpdate</type>
            </action>
        </rejectionActions>
    </approvalStep>
    <approvalStep>
        <allowDelegate>true</allowDelegate>
        <approvalActions>
            <action>
                <name>Stage_to_PSR</name>
                <type>FieldUpdate</type>
            </action>
        </approvalActions>
        <assignedApprover>
            <approver>
                <name>Customer_Network_Provisioning</name>
                <type>queue</type>
            </approver>
            <whenMultipleApprovers>FirstResponse</whenMultipleApprovers>
        </assignedApprover>
        <label>CNP Quality Assurance Review</label>
        <name>CNP_Quality_Assurance_Review</name>
        <rejectBehavior>
            <type>RejectRequest</type>
        </rejectBehavior>
        <rejectionActions>
            <action>
                <name>Owner_to_SE_Queue</name>
                <type>FieldUpdate</type>
            </action>
            <action>
                <name>Reject_to_SE</name>
                <type>FieldUpdate</type>
            </action>
        </rejectionActions>
    </approvalStep>
    <approvalStep>
        <allowDelegate>true</allowDelegate>
        <approvalActions>
            <action>
                <name>Owner_to_Network_COE_MS</name>
                <type>FieldUpdate</type>
            </action>
            <action>
                <name>Stage_to_Network_COE_DPI_Billing</name>
                <type>FieldUpdate</type>
            </action>
        </approvalActions>
        <assignedApprover>
            <approver>
                <name>Customer_Network_Provisioning</name>
                <type>queue</type>
            </approver>
            <whenMultipleApprovers>FirstResponse</whenMultipleApprovers>
        </assignedApprover>
        <label>PSR in Progress</label>
        <name>PSR_in_Progress</name>
        <rejectBehavior>
            <type>RejectRequest</type>
        </rejectBehavior>
    </approvalStep>
    <approvalStep>
        <allowDelegate>false</allowDelegate>
        <approvalActions>
            <action>
                <name>Stage_to_Network_COE_DPI_Billing</name>
                <type>FieldUpdate</type>
            </action>
        </approvalActions>
        <assignedApprover>
            <approver>
                <name>Network_COE_Managed_Services</name>
                <type>queue</type>
            </approver>
            <whenMultipleApprovers>FirstResponse</whenMultipleApprovers>
        </assignedApprover>
        <description>Network COE does DPI Billing portion of the order</description>
        <entryCriteria>
            <criteriaItems>
                <field>Order.Product_Type__c</field>
                <operation>equals</operation>
                <value>Managed Enterprise WiFi</value>
            </criteriaItems>
            <criteriaItems>
                <field>Order.Product__c</field>
                <operation>equals</operation>
                <value>Managed Enterprise WiFi - New or Existing Ethernet</value>
            </criteriaItems>
        </entryCriteria>
        <label>Network COE DPI Billing</label>
        <name>Network_COE_DPI_Billing</name>
        <rejectBehavior>
            <type>RejectRequest</type>
        </rejectBehavior>
    </approvalStep>
    <description>Create Date &lt;= 11/4/2021
MS - New/Upgrade Ethernet approval process
Cloud Managed Solutions,Managed Enterprise WiFi - New or Existing Ethernet)</description>
    <enableMobileDeviceAccess>true</enableMobileDeviceAccess>
    <entryCriteria>
        <booleanFilter>1 AND (2 AND 3) and 4</booleanFilter>
        <criteriaItems>
            <field>Order.RecordType</field>
            <operation>equals</operation>
            <value>New Order,Move Order,Upgrade/Downgrade Order</value>
        </criteriaItems>
        <criteriaItems>
            <field>Order.Product_Type__c</field>
            <operation>equals</operation>
            <value>Cloud Managed Solutions,Managed Enterprise WiFi</value>
        </criteriaItems>
        <criteriaItems>
            <field>Order.Product__c</field>
            <operation>equals</operation>
            <value>CMS - New or Upgrade Ethernet,Managed Enterprise WiFi - New or Existing Ethernet</value>
        </criteriaItems>
        <criteriaItems>
            <field>Order.CreatedDate</field>
            <operation>greaterOrEqual</operation>
            <value>11/4/2021 3:00 AM</value>
        </criteriaItems>
    </entryCriteria>
    <finalApprovalActions>
        <action>
            <name>Stage_to_Complete</name>
            <type>FieldUpdate</type>
        </action>
    </finalApprovalActions>
    <finalApprovalRecordLock>true</finalApprovalRecordLock>
    <finalRejectionActions>
        <action>
            <name>Reject_to_AE</name>
            <type>FieldUpdate</type>
        </action>
        <action>
            <name>Unlock_Order</name>
            <type>FieldUpdate</type>
        </action>
    </finalRejectionActions>
    <finalRejectionRecordLock>false</finalRejectionRecordLock>
    <initialSubmissionActions>
        <action>
            <name>Approve_to_SE</name>
            <type>FieldUpdate</type>
        </action>
        <action>
            <name>Owner_to_Sales_Engineering</name>
            <type>FieldUpdate</type>
        </action>
    </initialSubmissionActions>
    <label>CNP - M WiFi Ethernet New/Upgrd/Existng</label>
    <processOrder>11</processOrder>
    <recallActions>
        <action>
            <name>Unlock_Order</name>
            <type>FieldUpdate</type>
        </action>
    </recallActions>
    <recordEditability>AdminOrCurrentApprover</recordEditability>
    <showApprovalHistory>true</showApprovalHistory>
</ApprovalProcess>
