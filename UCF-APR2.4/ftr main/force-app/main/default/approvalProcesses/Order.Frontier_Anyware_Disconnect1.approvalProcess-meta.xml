<?xml version="1.0" encoding="UTF-8"?>
<ApprovalProcess xmlns="http://soap.sforce.com/2006/04/metadata">
    <active>false</active>
    <allowRecall>false</allowRecall>
    <allowedSubmitters>
        <type>owner</type>
    </allowedSubmitters>
    <approvalPageFields>
        <field>OrderNumber</field>
        <field>Owner</field>
    </approvalPageFields>
    <approvalStep>
        <allowDelegate>false</allowDelegate>
        <assignedApprover>
            <approver>
                <name>CPE_COE_Central</name>
                <type>queue</type>
            </approver>
            <whenMultipleApprovers>FirstResponse</whenMultipleApprovers>
        </assignedApprover>
        <entryCriteria>
            <criteriaItems>
                <field>Order.ShippingStateCode</field>
                <operation>equals</operation>
                <value>IL,IN,IA,MN,NE,OH,TX,WI</value>
            </criteriaItems>
            <criteriaItems>
                <field>Order.CPE_COE__c</field>
                <operation>equals</operation>
            </criteriaItems>
        </entryCriteria>
        <ifCriteriaNotMet>GotoNextStep</ifCriteriaNotMet>
        <label>CPECOE - Central</label>
        <name>CPECOE_Central</name>
    </approvalStep>
    <approvalStep>
        <allowDelegate>false</allowDelegate>
        <assignedApprover>
            <approver>
                <name>CPE_COE_East</name>
                <type>queue</type>
            </approver>
            <whenMultipleApprovers>FirstResponse</whenMultipleApprovers>
        </assignedApprover>
        <entryCriteria>
            <criteriaItems>
                <field>Order.ShippingStateCode</field>
                <operation>equals</operation>
                <value>AL,CT,FL,GA,MS,NY,NC,PA,SC,TN,WV</value>
            </criteriaItems>
            <criteriaItems>
                <field>Order.CPE_COE__c</field>
                <operation>equals</operation>
            </criteriaItems>
        </entryCriteria>
        <ifCriteriaNotMet>GotoNextStep</ifCriteriaNotMet>
        <label>CPE COE - East</label>
        <name>CPE_COE_East</name>
        <rejectBehavior>
            <type>RejectRequest</type>
        </rejectBehavior>
    </approvalStep>
    <approvalStep>
        <allowDelegate>false</allowDelegate>
        <assignedApprover>
            <approver>
                <name>CPE_COE_West</name>
                <type>queue</type>
            </approver>
            <whenMultipleApprovers>FirstResponse</whenMultipleApprovers>
        </assignedApprover>
        <entryCriteria>
            <criteriaItems>
                <field>Order.ShippingStateCode</field>
                <operation>equals</operation>
                <value>AZ,CA,ID,MT,NV,NM,OR,UT,WA</value>
            </criteriaItems>
            <criteriaItems>
                <field>Order.CPE_COE__c</field>
                <operation>equals</operation>
            </criteriaItems>
        </entryCriteria>
        <label>CPECOE - West</label>
        <name>CPECOE_West</name>
        <rejectBehavior>
            <type>RejectRequest</type>
        </rejectBehavior>
    </approvalStep>
    <enableMobileDeviceAccess>true</enableMobileDeviceAccess>
    <entryCriteria>
        <criteriaItems>
            <field>Order.Product__c</field>
            <operation>equals</operation>
            <value>Frontier AnyWare UCaaS,Frontier AnyWare UCaaS - BYOA,Frontier AnyWare UCaaS + Ethernet,Frontier AnyWare UCaaS + Broadband</value>
        </criteriaItems>
        <criteriaItems>
            <field>Order.RecordType</field>
            <operation>equals</operation>
            <value>Disconnect Order</value>
        </criteriaItems>
    </entryCriteria>
    <finalApprovalActions>
        <action>
            <name>Stage_to_Complete</name>
            <type>FieldUpdate</type>
        </action>
    </finalApprovalActions>
    <finalApprovalRecordLock>true</finalApprovalRecordLock>
    <finalRejectionActions>
        <action>
            <name>Reject_to_AE</name>
            <type>FieldUpdate</type>
        </action>
    </finalRejectionActions>
    <finalRejectionRecordLock>false</finalRejectionRecordLock>
    <initialSubmissionActions>
        <action>
            <name>Lock_Order</name>
            <type>FieldUpdate</type>
