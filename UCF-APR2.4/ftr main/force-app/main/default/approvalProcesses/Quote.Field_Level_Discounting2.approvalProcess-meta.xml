<?xml version="1.0" encoding="UTF-8"?>
<ApprovalProcess xmlns="http://soap.sforce.com/2006/04/metadata">
    <active>true</active>
    <allowRecall>true</allowRecall>
    <allowedSubmitters>
        <type>owner</type>
    </allowedSubmitters>
    <allowedSubmitters>
        <submitter>Alternate_Channel</submitter>
        <type>roleSubordinatesInternal</type>
    </allowedSubmitters>
    <allowedSubmitters>
        <submitter>Carrier_Wholesale_Operations</submitter>
        <type>roleSubordinatesInternal</type>
    </allowedSubmitters>
    <allowedSubmitters>
        <submitter>Commercial</submitter>
        <type>roleSubordinatesInternal</type>
    </allowedSubmitters>
    <allowedSubmitters>
        <submitter>Frontier_Secure</submitter>
        <type>roleSubordinatesInternal</type>
    </allowedSubmitters>
    <allowedSubmitters>
        <submitter>Sales_Business_Operations</submitter>
        <type>roleSubordinatesInternal</type>
    </allowedSubmitters>
    <allowedSubmitters>
        <submitter>Sales_Support_Director</submitter>
        <type>roleSubordinatesInternal</type>
    </allowedSubmitters>
    <allowedSubmitters>
        <submitter>Testing_Management</submitter>
        <type>roleSubordinatesInternal</type>
    </allowedSubmitters>
    <approvalPageFields>
        <field>Name</field>
        <field>QuoteNumber</field>
        <field>Opportunity</field>
        <field>Account</field>
        <field>Contact</field>
        <field>Approval_Status__c</field>
        <field>Approval_Status_Date__c</field>
        <field>vlocity_cmt__EffectiveRecurringTotal__c</field>
        <field>vlocity_cmt__EffectiveOneTimeTotal__c</field>
    </approvalPageFields>
    <approvalStep>
        <allowDelegate>false</allowDelegate>
        <assignedApprover>
            <approver>
                <name>Owner</name>
                <type>relatedUserField</type>
            </approver>
            <whenMultipleApprovers>FirstResponse</whenMultipleApprovers>
        </assignedApprover>
        <entryCriteria>
            <formula>false</formula>
        </entryCriteria>
        <ifCriteriaNotMet>GotoNextStep</ifCriteriaNotMet>
        <label>Step 1</label>
        <name>Step_1</name>
    </approvalStep>
    <approvalStep>
        <allowDelegate>false</allowDelegate>
        <assignedApprover>
            <approver>
                <name>RSM_Approver__c</name>
                <type>relatedUserField</type>
            </approver>
            <whenMultipleApprovers>FirstResponse</whenMultipleApprovers>
        </assignedApprover>
        <entryCriteria>
            <booleanFilter>1 OR 2 OR 3</booleanFilter>
            <criteriaItems>
                <field>Quote.ftr_Approval_Type__c</field>
                <operation>equals</operation>
                <value>RSM_APPROVAL</value>
            </criteriaItems>
            <criteriaItems>
                <field>Quote.ftr_Approval_Type__c</field>
                <operation>equals</operation>
                <value>VP_APPROVAL</value>
            </criteriaItems>
            <criteriaItems>
                <field>Quote.ftr_Approval_Type__c</field>
                <operation>equals</operation>
                <value>Approval 1</value>
            </criteriaItems>
        </entryCriteria>
        <ifCriteriaNotMet>ApproveRecord</ifCriteriaNotMet>
        <label>RSM Approval</label>
        <name>Step_2</name>
        <rejectBehavior>
            <type>RejectRequest</type>
        </rejectBehavior>
    </approvalStep>
    <approvalStep>
        <allowDelegate>false</allowDelegate>
        <assignedApprover>
            <approver>
                <name>VP_Approver__c</name>
                <type>relatedUserField</type>
            </approver>
            <whenMultipleApprovers>FirstResponse</whenMultipleApprovers>
        </assignedApprover>
        <description>VP Approver</description>
        <entryCriteria>
            <criteriaItems>
                <field>Quote.ftr_Approval_Type__c</field>
                <operation>equals</operation>
                <value>VP_APPROVAL</value>
            </criteriaItems>
        </entryCriteria>
        <label>VP Approval</label>
        <name>step_3</name>
        <rejectBehavior>
            <type>BackToPrevious</type>
        </rejectBehavior>
        <rejectionActions>
            <action>
                <name>Final_Rejection_Email_Alert</name>
                <type>Alert</type>
            </action>
        </rejectionActions>
    </approvalStep>
    <emailTemplate>unfiled$public/ICB_Quote_Email</emailTemplate>
    <enableMobileDeviceAccess>false</enableMobileDeviceAccess>
    <entryCriteria>
        <criteriaItems>
            <field>Quote.Approval_Status__c</field>
            <operation>equals</operation>
            <value>Approval Required</value>
        </criteriaItems>
        <criteriaItems>
            <field>Quote.ftr_Approval_Type__c</field>
            <operation>notEqual</operation>
            <value>null</value>
        </criteriaItems>
    </entryCriteria>
    <finalApprovalActions>
        <action>
            <name>Approval_Status_Approved</name>
            <type>FieldUpdate</type>
        </action>
        <action>
            <name>Quote_Final_Approval_Date</name>
            <type>FieldUpdate</type>
        </action>
        <action>
            <name>Set_Approval_Status_Date</name>
            <type>FieldUpdate</type>
        </action>
        <action>
            <name>Status_Approved</name>
            <type>FieldUpdate</type>
        </action>
        <action>
            <name>Vlocity_Quote_FLA_Approved</name>
            <type>Alert</type>
        </action>
    </finalApprovalActions>
    <finalApprovalRecordLock>false</finalApprovalRecordLock>
    <finalRejectionActions>
        <action>
            <name>Approval_Status_Denied</name>
            <type>FieldUpdate</type>
        </action>
        <action>
            <name>Quote_Final_Rejection_Date</name>
            <type>FieldUpdate</type>
        </action>
        <action>
            <name>Quote_Status_Rejected</name>
            <type>FieldUpdate</type>
        </action>
        <action>
            <name>Set_Approval_Status_Date</name>
            <type>FieldUpdate</type>
        </action>
        <action>
            <name>Vlocity_Quote_FLA_Rejected</name>
            <type>Alert</type>
        </action>
    </finalRejectionActions>
    <finalRejectionRecordLock>false</finalRejectionRecordLock>
    <initialSubmissionActions>
        <action>
            <name>Quote_Status_In_Review</name>
            <type>FieldUpdate</type>
        </action>
        <action>
            <name>Quote_Status_Pending_Approval</name>
            <type>Alert</type>
        </action>
        <action>
            <name>Set_Approval_Status_Date</name>
            <type>FieldUpdate</type>
        </action>
    </initialSubmissionActions>
    <label>Field Level Discounting2</label>
    <processOrder>3</processOrder>
    <recallActions>
        <action>
            <name>Approval_Status_Approval_Required</name>
            <type>FieldUpdate</type>
        </action>
        <action>
            <name>Quote_Status_Needs_Review</name>
            <type>FieldUpdate</type>
        </action>
    </recallActions>
    <recordEditability>AdminOrCurrentApprover</recordEditability>
    <showApprovalHistory>false</showApprovalHistory>
</ApprovalProcess>
