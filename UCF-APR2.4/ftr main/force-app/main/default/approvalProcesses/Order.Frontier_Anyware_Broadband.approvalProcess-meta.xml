<?xml version="1.0" encoding="UTF-8"?>
<ApprovalProcess xmlns="http://soap.sforce.com/2006/04/metadata">
    <active>false</active>
    <allowRecall>false</allowRecall>
    <allowedSubmitters>
        <type>owner</type>
    </allowedSubmitters>
    <approvalPageFields>
        <field>OrderNumber</field>
        <field>Owner</field>
    </approvalPageFields>
    <approvalStep>
        <allowDelegate>false</allowDelegate>
        <approvalActions>
            <action>
                <name>Stage_to_In_Progress</name>
                <type>FieldUpdate</type>
            </action>
        </approvalActions>
        <assignedApprover>
            <approver>
                <type>adhoc</type>
            </approver>
        </assignedApprover>
        <entryCriteria>
            <criteriaItems>
                <field>Order.Broadband_COE__c</field>
                <operation>equals</operation>
            </criteriaItems>
        </entryCriteria>
        <ifCriteriaNotMet>GotoNextStep</ifCriteriaNotMet>
        <label>Broadband Assignment</label>
        <name>Broadband_Assignment</name>
    </approvalStep>
    <approvalStep>
        <allowDelegate>false</allowDelegate>
        <assignedApprover>
            <approver>
                <name>CPE_DCS</name>
                <type>queue</type>
            </approver>
            <approver>
                <name>Hot_Cuts_Frontier_Anyware</name>
                <type>queue</type>
            </approver>
            <approver>
                <name>VoIP_Translations</name>
                <type>queue</type>
            </approver>
            <whenMultipleApprovers>Unanimous</whenMultipleApprovers>
        </assignedApprover>
        <entryCriteria>
            <formula>OR( 
ISBLANK(VoIP_Translations__c ), 
ISBLANK(Hot_Cut__c), 
ISBLANK(CPE_DCS__c) 
)</formula>
        </entryCriteria>
        <ifCriteriaNotMet>GotoNextStep</ifCriteriaNotMet>
        <label>Assignment</label>
        <name>Assignment</name>
        <rejectBehavior>
            <type>RejectRequest</type>
        </rejectBehavior>
    </approvalStep>
    <approvalStep>
        <allowDelegate>false</allowDelegate>
        <assignedApprover>
            <approver>
                <name>CPE_COE_Central</name>
                <type>queue</type>
            </approver>
            <whenMultipleApprovers>FirstResponse</whenMultipleApprovers>
        </assignedApprover>
        <entryCriteria>
            <criteriaItems>
                <field>Order.ShippingStateCode</field>
                <operation>equals</operation>
                <value>IL,IN,IA,MN,NE,OH,TX,WI</value>
            </criteriaItems>
            <criteriaItems>
                <field>Order.CPE_COE__c</field>
                <operation>equals</operation>
            </criteriaItems>
        </entryCriteria>
        <ifCriteriaNotMet>GotoNextStep</ifCriteriaNotMet>
        <label>CPECOE - Central</label>
        <name>CPECOE_Central</name>
        <rejectBehavior>
            <type>RejectRequest</type>
        </rejectBehavior>
    </approvalStep>
    <approvalStep>
        <allowDelegate>false</allowDelegate>
        <assignedApprover>
            <approver>
                <name>CPE_COE_East</name>
                <type>queue</type>
            </approver>
            <whenMultipleApprovers>FirstResponse</whenMultipleApprovers>
        </assignedApprover>
        <entryCriteria>
            <criteriaItems>
                <field>Order.ShippingStateCode</field>
                <operation>equals</operation>
                <value>AL,CT,FL,GA,MS,NY,NC,PA,SC,TN,WV</value>
            </criteriaItems>
            <criteriaItems>
                <field>Order.CPE_COE__c</field>
                <operation>equals</operation>
            </criteriaItems>
        </entryCriteria>
        <ifCriteriaNotMet>GotoNextStep</ifCriteriaNotMet>
        <label>CPE COE - East</label>
        <name>CPE_COE_East</name>
        <rejectBehavior>
            <type>RejectRequest</type>
        </rejectBehavior>
    </approvalStep>
    <approvalStep>
        <allowDelegate>false</allowDelegate>
        <assignedApprover>
            <approver>
                <name>CPE_COE_West</name>
                <type>queue</type>
            </approver>
            <whenMultipleApprovers>FirstResponse</whenMultipleApprovers>
        </assignedApprover>
        <entryCriteria>
            <criteriaItems>
                <field>Order.ShippingStateCode</field>
                <operation>equals</operation>
                <value>AZ,CA,ID,MT,NV,NM,OR,UT,WA</value>
            </criteriaItems>
            <criteriaItems>
                <field>Order.CPE_COE__c</field>
                <operation>equals</operation>
            </criteriaItems>
        </entryCriteria>
        <ifCriteriaNotMet>GotoNextStep</ifCriteriaNotMet>
        <label>CPECOE - West</label>
        <name>CPECOE_West</name>
        <rejectBehavior>
            <type>RejectRequest</type>
        </rejectBehavior>
    </approvalStep>
    <approvalStep>
        <allowDelegate>false</allowDelegate>
        <assignedApprover>
            <approver>
                <name>VoIP_Translations__c</name>
                <type>relatedUserField</type>
            </approver>
            <approver>
                <name>Hot_Cut__c</name>
                <type>relatedUserField</type>
            </approver>
            <approver>
                <name>CPE_COE__c</name>
                <type>relatedUserField</type>
            </approver>
            <approver>
                <name>Broadband_COE__c</name>
                <type>relatedUserField</type>
            </approver>
            <approver>
                <name>CPE_DCS__c</name>
                <type>relatedUserField</type>
            </approver>
            <whenMultipleApprovers>Unanimous</whenMultipleApprovers>
        </assignedApprover>
        <label>Order Complete</label>
        <name>Order_Complete</name>
        <rejectBehavior>
            <type>RejectRequest</type>
        </rejectBehavior>
    </approvalStep>
    <enableMobileDeviceAccess>false</enableMobileDeviceAccess>
    <entryCriteria>
        <criteriaItems>
            <field>Order.Product__c</field>
            <operation>equals</operation>
            <value>Frontier AnyWare UCaaS + Broadband</value>
        </criteriaItems>
        <criteriaItems>
            <field>Order.On_Hold__c</field>
            <operation>equals</operation>
            <value>Frontier Anyware w/ Broadband</value>
        </criteriaItems>
        <criteriaItems>
            <field>Order.RecordType</field>
            <operation>equals</operation>
            <value>Change Order,Move Order,New Order,Upgrade/Downgrade Order</value>
        </criteriaItems>
        <criteriaItems>
            <field>Order.UcaaS_Cutover_Flag__c</field>
            <operation>equals</operation>
            <value>False</value>
        </criteriaItems>
    </entryCriteria>
    <finalApprovalActions>
        <action>
            <name>Stage_to_Complete</name>
            <type>FieldUpdate</type>
        </action>
    </finalApprovalActions>
    <finalApprovalRecordLock>true</finalApprovalRecordLock>
    <finalRejectionActions>
        <action>
            <name>Reject_to_AE</name>
            <type>FieldUpdate</type>
        </action>
    </finalRejectionActions>
    <finalRejectionRecordLock>false</finalRejectionRecordLock>
    <initialSubmissionActions>
        <action>
            <name>Lock_Order</name>
            <type>FieldUpdate</type>
        </action>
        <action>
            <name>Stage_to_Assignment</name>
            <type>FieldUpdate</type>
        </action>
    </initialSubmissionActions>
    <label>Frontier Anyware + Broadband</label>
    <processOrder>4</processOrder>
    <recallActions>
        <action>
            <name>Reject_to_AE</name>
            <type>FieldUpdate</type>
        </action>
    </recallActions>
    <recordEditability>AdminOrCurrentApprover</recordEditability>
    <showApprovalHistory>true</showApprovalHistory>
</ApprovalProcess>
