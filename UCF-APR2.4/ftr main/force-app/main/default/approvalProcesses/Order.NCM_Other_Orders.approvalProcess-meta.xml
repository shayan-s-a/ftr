<?xml version="1.0" encoding="UTF-8"?>
<ApprovalProcess xmlns="http://soap.sforce.com/2006/04/metadata">
    <active>false</active>
    <allowRecall>true</allowRecall>
    <allowedSubmitters>
        <type>owner</type>
    </allowedSubmitters>
    <approvalPageFields>
        <field>OrderNumber</field>
        <field>Name</field>
        <field>Owner</field>
        <field>Order_Stage__c</field>
        <field>Order_Created_Date__c</field>
        <field>Reject_Reason__c</field>
    </approvalPageFields>
    <approvalStep>
        <allowDelegate>true</allowDelegate>
        <approvalActions>
            <action>
                <name>Owner_to_NCM</name>
                <type>FieldUpdate</type>
            </action>
            <action>
                <name>Stage_to_PSR</name>
                <type>FieldUpdate</type>
            </action>
        </approvalActions>
        <assignedApprover>
            <approver>
                <name>Network_Cost_Management</name>
                <type>queue</type>
            </approver>
            <whenMultipleApprovers>FirstResponse</whenMultipleApprovers>
        </assignedApprover>
        <label>Network Cost Management</label>
        <name>Network_Cost_Management</name>
        <rejectionActions>
            <action>
                <name>Reject_to_AE</name>
                <type>FieldUpdate</type>
            </action>
        </rejectionActions>
    </approvalStep>
    <approvalStep>
        <allowDelegate>false</allowDelegate>
        <assignedApprover>
            <approver>
                <name>Network_Cost_Management</name>
                <type>queue</type>
            </approver>
            <whenMultipleApprovers>FirstResponse</whenMultipleApprovers>
        </assignedApprover>
        <label>PSR in Progress</label>
        <name>PSR_in_Progress</name>
        <rejectBehavior>
            <type>RejectRequest</type>
        </rejectBehavior>
    </approvalStep>
    <description>Records Only Order,Transfer Ownership Order,Contract Renewal,Disconnect Order</description>
    <enableMobileDeviceAccess>true</enableMobileDeviceAccess>
    <entryCriteria>
        <criteriaItems>
            <field>Order.RecordType</field>
            <operation>equals</operation>
            <value>Records Only Order,Transfer Ownership Order,Contract Renewal,Disconnect Order</value>
        </criteriaItems>
        <criteriaItems>
            <field>Order.CreatedDate</field>
            <operation>lessThan</operation>
            <value>5/16/2021 9:00 PM</value>
        </criteriaItems>
        <criteriaItems>
            <field>Order.Product__c</field>
            <operation>equals</operation>
            <value>E-Reach,Off-Net Ethernet</value>
        </criteriaItems>
    </entryCriteria>
    <finalApprovalActions>
        <action>
            <name>Stage_to_Complete</name>
            <type>FieldUpdate</type>
        </action>
    </finalApprovalActions>
    <finalApprovalRecordLock>true</finalApprovalRecordLock>
    <finalRejectionActions>
        <action>
            <name>Order_Rejected</name>
            <type>Alert</type>
        </action>
        <action>
            <name>Reject_to_AE</name>
            <type>FieldUpdate</type>
        </action>
    </finalRejectionActions>
    <finalRejectionRecordLock>false</finalRejectionRecordLock>
    <initialSubmissionActions>
        <action>
            <name>Owner_to_NCM</name>
            <type>FieldUpdate</type>
        </action>
        <action>
            <name>Stage_to_NCM</name>
            <type>FieldUpdate</type>
        </action>
    </initialSubmissionActions>
    <label>NCM-Other Orders</label>
    <processOrder>13</processOrder>
    <recordEditability>AdminOrCurrentApprover</recordEditability>
    <showApprovalHistory>true</showApprovalHistory>
</ApprovalProcess>
