    <rules>
        <fullName>Discount Type %3D ICBA or CTRA</fullName>
        <actions>
            <name>CurrentStatus_ICB_Approved</name>
            <type>FieldUpdate</type>
        </actions>
        <actions>
            <name>Overall_Status_Approved</name>
            <type>FieldUpdate</type>
        </actions>
        <active>true</active>
        <criteriaItems>
            <field>SterlingQuoteItem__c.DiscountType__c</field>
            <operation>equals</operation>
            <value>ICBA</value>
        </criteriaItems>
        <triggerType>onCreateOrTriggeringUpdate</triggerType>
    </rules>
    <rules>
        <fullName>Inactive Status Update</fullName>
        <active>true</active>
        <description>Change status to inactive if status of approval hasn&apos;t changed in 30 days</description>
        <formula>And(    ISPICKVAL(OverallStatus__c, &apos;Pending&apos;) ,    Status_Date__c = Today() )</formula>
        <triggerType>onCreateOrTriggeringUpdate</triggerType>
        <workflowTimeTriggers>
            <actions>
                <name>Email_Alert_ICB_Inactive</name>
                <type>Alert</type>
            </actions>
            <actions>
                <name>Set_Status_Inactive</name>
                <type>FieldUpdate</type>
            </actions>
            <timeLength>30</timeLength>
            <workflowTimeTriggerUnit>Days</workflowTimeTriggerUnit>
        </workflowTimeTriggers>
    </rules>
    <rules>
        <fullName>Item in Jeopardy</fullName>
        <active>true</active>
        <description>Set true when Status date hasn&apos;t changed in 3 days.</description>
        <formula>And(     ISPICKVAL( OverallStatus__c , &apos;Pending&apos;),     Status_Date__c = Today()  )</formula>
        <triggerType>onCreateOrTriggeringUpdate</triggerType>
        <workflowTimeTriggers>
            <actions>
                <name>Jeopardy_Email_Alert</name>
                <type>Alert</type>
            </actions>
            <actions>
                <name>Jeopardy_True</name>
                <type>FieldUpdate</type>
            </actions>
            <offsetFromField>SterlingQuoteItem__c.Status_Date__c</offsetFromField>
            <timeLength>3</timeLength>
            <workflowTimeTriggerUnit>Days</workflowTimeTriggerUnit>
        </workflowTimeTriggers>
    </rules>
    <rules>
        <fullName>Jeopardy Update</fullName>
        <actions>
            <name>Jeopardy_Update</name>
            <type>FieldUpdate</type>
        </actions>
        <active>true</active>
        <criteriaItems>
            <field>SterlingQuoteItem__c.OverallStatus__c</field>
            <operation>equals</operation>
            <value>Abandoned,Accepted,Approved,Rejected,Recalled</value>
        </criteriaItems>
        <criteriaItems>
            <field>SterlingQuoteItem__c.Jeopardy__c</field>
            <operation>equals</operation>
            <value>True</value>
        </criteriaItems>
        <triggerType>onAllChanges</triggerType>
    </rules>
    <rules>
        <fullName>Quote Item DSAT field</fullName>
        <active>false</active>
        <criteriaItems>
            <field>SterlingQuoteItem__c.DSAT_AddressID__c</field>
            <operation>notEqual</operation>
        </criteriaItems>
        <triggerType>onAllChanges</triggerType>
    </rules>
    <rules>
        <fullName>Round VAlues</fullName>
        <actions>
            <name>Round_Adjusted_MRC</name>
            <type>FieldUpdate</type>
        </actions>
        <actions>
            <name>Round_Adjusted_NRC</name>
            <type>FieldUpdate</type>
        </actions>
        <actions>
            <name>Round_Standard_MRC</name>
            <type>FieldUpdate</type>
        </actions>
        <actions>
            <name>Round_Standard_NRC</name>
            <type>FieldUpdate</type>
        </actions>
        <active>true</active>
        <formula>OR( ISNEW(), ISCHANGED( AdjustedMRC__c ), ISCHANGED( AdjustedNRC__c )  )</formula>
        <triggerType>onAllChanges</triggerType>
    </rules>
    <rules>
        <fullName>Send Email For Managed Firewall Discount</fullName>
        <actions>
            <name>Discount_IS_Added_For_Managed_Firewall</name>
            <type>Alert</type>
        </actions>
        <active>true</active>
        <criteriaItems>
            <field>SterlingQuoteItem__c.Product_Name_Quote__c</field>
            <operation>startsWith</operation>
            <value>MF</value>
        </criteriaItems>
        <criteriaItems>
            <field>SterlingQuoteItem__c.DiscountAmount__c</field>
            <operation>lessThan</operation>
            <value>0</value>
        </criteriaItems>
        <description>Send Email For Managed Firewall Discount</description>
        <triggerType>onCreateOrTriggeringUpdate</triggerType>
    </rules>
</Workflow>
