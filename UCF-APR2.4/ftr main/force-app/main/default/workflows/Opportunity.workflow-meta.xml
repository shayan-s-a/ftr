    <rules>
        <fullName>Check Closed%2FWon Opportunity</fullName>
        <active>false</active>
        <criteriaItems>
            <field>Opportunity.StageName</field>
            <operation>equals</operation>
            <value>Closed/Won,Closed/Lost</value>
        </criteriaItems>
        <criteriaItems>
            <field>Opportunity.RecordTypeId</field>
            <operation>equals</operation>
            <value>Opportunity Lightning</value>
        </criteriaItems>
        <description>If Opportunity Stage=Closed/won or lost then agent should&apos;nt be able to create quote.</description>
        <triggerType>onCreateOrTriggeringUpdate</triggerType>
    </rules>
    <rules>
        <fullName>Close Opportunity for PST profile</fullName>
        <actions>
            <name>Update_Other_Reason_Lost_Details</name>
            <type>FieldUpdate</type>
        </actions>
        <actions>
            <name>Update_Reason_Lost</name>
            <type>FieldUpdate</type>
        </actions>
        <actions>
            <name>Update_Sales_Stage_to_Closed_Lost</name>
            <type>FieldUpdate</type>
        </actions>
        <active>false</active>
        <description>Update Opportunities for Partner Support Team profile if the Opportunity is greater than Created Date  &gt;60 and Sales Stage is not equal to Closed/Won or Closed/Lost</description>
        <formula>AND( (RecordTypeId = &apos;012E0000000dgAK&apos;), (Owner.ProfileId = &apos;00e0L000000Qtux&apos;), ( TODAY() - DATEVALUE(CreatedDate) &gt; 60), (OR( NOT(ISPICKVAL( StageName,&apos;Closed/Won&apos;)),  NOT(ISPICKVAL( StageName,&apos;Closed/Lost&apos;)))))</formula>
        <triggerType>onAllChanges</triggerType>
    </rules>
