    <description>Checks to see if the follow up date is a weekend and then changes it to the following Monday.</description>
    <formulas>
        <processMetadataValues>
            <name>originalFormula</name>
            <value>
                <stringValue>CASE(
MOD(DATEVALUE( [Case].CreatedDate)  - DATE( 1900, 1, 7 ), 7 ),

5,[Case].CreatedDate + 3,
6, [Case].CreatedDate + 2,
[Case].CreatedDate + 1
)</stringValue>
            </value>
        </processMetadataValues>
