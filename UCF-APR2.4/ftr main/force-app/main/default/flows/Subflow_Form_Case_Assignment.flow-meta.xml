                    <booleanValue>true</booleanValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Set_Unassigned_CSAM_Queue</targetReference>
            </connector>
            <label>No CSAM</label>
        </rules>
    </decisions>
