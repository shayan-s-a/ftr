    <decisions>
        <name>Account_NAT_or_FEDERAL</name>
        <label>Account NAT or FEDERAL?</label>
        <locationX>281</locationX>
        <locationY>674</locationY>
        <defaultConnector>
            <targetReference>Is_CSAM_in_Account_Team</targetReference>
        </defaultConnector>
        <defaultConnectorLabel>No</defaultConnectorLabel>
        <rules>
            <name>YesAccountNATOrFederal</name>
            <conditionLogic>1 AND ( 2 OR 3 OR 4 OR 5 )</conditionLogic>
            <conditions>
                <leftValueReference>AccountRecord</leftValueReference>
                <operator>IsNull</operator>
                <rightValue>
                    <booleanValue>false</booleanValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>AccountRecord.Type</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>FEDERAL</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>AccountRecord.Federal__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <booleanValue>true</booleanValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>AccountRecord.Owner.UserRole.Name</leftValueReference>
                <operator>StartsWith</operator>
                <rightValue>
                    <stringValue>National</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>AccountRecord.Owner.UserRole.Name</leftValueReference>
                <operator>StartsWith</operator>
                <rightValue>
                    <stringValue>Federal</stringValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Set_Assigned_CSAM_Queue</targetReference>
            </connector>
            <label>Yes</label>
        </rules>
    </decisions>
