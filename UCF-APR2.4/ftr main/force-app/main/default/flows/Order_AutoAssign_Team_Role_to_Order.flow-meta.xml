                <rightValue>
                    <booleanValue>false</booleanValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Is_CNP_Active</targetReference>
            </connector>
            <label>CNP is found</label>
        </rules>
    </decisions>
