    <decisions>
        <description>Skip audit case for alternate channel opportunities.</description>
        <name>Alternate_Channel_Opty</name>
        <label>Alternate Channel Opty?</label>
        <locationX>456</locationX>
        <locationY>276</locationY>
        <defaultConnector>
            <targetReference>Net_New_MRR_CPE</targetReference>
        </defaultConnector>
        <defaultConnectorLabel>Default Outcome</defaultConnectorLabel>
        <rules>
            <name>Alternate_Channel</name>
            <conditionLogic>1 OR 2 OR 3 OR (4 AND 5)</conditionLogic>
            <conditions>
                <leftValueReference>$Record.Owner.UserRole.Name</leftValueReference>
                <operator>Contains</operator>
                <rightValue>
                    <stringValue>B2B - Alt Channel</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>$Record.Owner.Manager.UserRole.Name</leftValueReference>
                <operator>Contains</operator>
                <rightValue>
                    <stringValue>B2B - Alt Channel</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>$Record.RVP__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>RVP Wholesale</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>$Record.RVP__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>RVP Alt Channel</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>$Record.RVP__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>RVP Customer Care</stringValue>
                </rightValue>
            </conditions>
            <label>Alternate Channel</label>
        </rules>
    </decisions>
