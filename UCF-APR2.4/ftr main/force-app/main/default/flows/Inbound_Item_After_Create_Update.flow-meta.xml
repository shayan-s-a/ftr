    <description>For adding Campaign Member Status based on Inbound Item</description>
    <environments>Default</environments>
    <formulas>
        <name>FinalANI</name>
        <dataType>String</dataType>
        <expression>LEFT({!RemovedSpecialChar}, 3) &amp; &quot;-&quot; &amp; RIGHT(LEFT({!RemovedSpecialChar},6), 3) &amp; &quot;-&quot; &amp; RIGHT({!RemovedSpecialChar}, {!LengthOfANI}-6)</expression>
    </formulas>
    <formulas>
        <name>IsCreated</name>
        <dataType>Boolean</dataType>
        <expression>ISNEW()</expression>
    </formulas>
    <formulas>
        <name>IsOwnedByQueue</name>
        <dataType>Boolean</dataType>
        <expression>BEGINS({!$Record.OwnerId}, &quot;00G&quot;)</expression>
    </formulas>
    <formulas>
        <name>LengthOfANI</name>
        <dataType>Number</dataType>
        <expression>LEN({!RemovedSpecialChar})</expression>
        <scale>0</scale>
    </formulas>
    <formulas>
        <name>RemovedSpecialChar</name>
        <dataType>String</dataType>
        <expression>SUBSTITUTE(SUBSTITUTE(SUBSTITUTE(SUBSTITUTE({!$Record.ANI__c}, &apos;(&apos;, &apos;&apos;), &apos;)&apos;, &apos;&apos;), &apos;-&apos;, &apos;&apos;), &apos; &apos;, &apos;&apos;)</expression>
    </formulas>
