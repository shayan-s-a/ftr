<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Only_Numbers_for_Mobile</fullName>
    <active>true</active>
    <errorConditionFormula>NOT(REGEX(MobilePhone,&quot;[0-9+\\-\\(\\)\\s]*&quot;))</errorConditionFormula>
    <errorDisplayField>MobilePhone</errorDisplayField>
    <errorMessage>Only Numbers allowed</errorMessage>
</ValidationRule>
