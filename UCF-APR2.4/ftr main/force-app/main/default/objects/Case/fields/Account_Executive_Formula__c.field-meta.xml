<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Account_Executive_Formula__c</fullName>
    <description>Displays the Account Executive that owns the associated account.</description>
    <externalId>false</externalId>
    <formula>IF(Account.Owner.FirstName != null,
Account.Owner.FirstName + &quot; &quot; +  Account.Owner.LastName,
Account.Owner.LastName
)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <inlineHelpText>Displays the Account Executive that owns the associated account.</inlineHelpText>
    <label>Account Executive</label>
    <required>false</required>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Text</type>
    <unique>false</unique>
</CustomField>
