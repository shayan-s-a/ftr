<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>CRDD_date_cannot_be_changed</fullName>
    <active>true</active>
    <errorConditionFormula>ISCHANGED(CRDD__c)</errorConditionFormula>
