<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>NPA_NXX_To_Mirror_Wire_Center_Format</fullName>
    <active>true</active>
    <errorConditionFormula>AND(NOT(ISBLANK(NPA_NXX_To_Mirror_Wire_Center__c)),
NOT(REGEX(NPA_NXX_To_Mirror_Wire_Center__c,&quot;[0-9]{6}&quot;)))</errorConditionFormula>
    <errorDisplayField>NPA_NXX_To_Mirror_Wire_Center__c</errorDisplayField>
    <errorMessage>The NPA/NXX Wire Center must be 6 numeric digits (Example: 123456)</errorMessage>
</ValidationRule>
