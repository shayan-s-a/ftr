<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Change_order_cant_close</fullName>
    <active>true</active>
    <errorConditionFormula>AND(
ISPICKVAL(Status, &quot;Closed&quot;),
ISBLANK(ChangeOrder_DateReturnedToCOE__c),
RecordType.Name = &quot;Change Order&quot;
)</errorConditionFormula>
    <errorMessage>Cannot close this case without populating the Date Returned to COE</errorMessage>
</ValidationRule>
