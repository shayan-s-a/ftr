<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Audit_Adjusted</fullName>
    <active>false</active>
    <errorConditionFormula>AND(
$RecordType.Name = &quot;Audit&quot;,
ISPICKVAL(Status, &quot;Adjusted&quot;),
ISBLANK(TEXT(Audit_Findings__c)))</errorConditionFormula>
    <errorMessage>Audit Findings is required</errorMessage>
</ValidationRule>
