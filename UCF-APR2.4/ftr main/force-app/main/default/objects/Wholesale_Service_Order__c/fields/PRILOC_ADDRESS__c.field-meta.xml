<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>PRILOC_ADDRESS__c</fullName>
    <description>Concatenate Priloc address fields to show complete address</description>
    <externalId>false</externalId>
    <formula>(IF(ISBLANK(PRILOC_ST_NUMBER__c), &apos;&apos;,PRILOC_ST_NUMBER__c &amp; &apos; &apos;) &amp;
IF(ISBLANK(PRILOC_ST_NAME__c), &apos;&apos;,PRILOC_ST_NAME__c &amp; &apos;, &apos;) &amp;
IF(ISBLANK(PRILOC_CITY__c), &apos;&apos;,PRILOC_CITY__c &amp; &apos;, &apos;) &amp;
IF(ISBLANK(PRILOC_STATE__c), &apos;&apos;,PRILOC_STATE__c &amp; &apos;, &apos;) &amp;
IF(ISBLANK( PRILOC_ZIP__c), &apos;&apos;,PRILOC_ZIP__c))</formula>
    <label>PRILOC - Address</label>
    <required>false</required>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Text</type>
    <unique>false</unique>
</CustomField>
