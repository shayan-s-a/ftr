<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>ETF_Amount_is_required</fullName>
    <active>true</active>
    <errorConditionFormula>AND( 
ISPICKVAL( Does_ETF_Apply__c ,&quot;Yes&quot;),
ISBLANK( ETF_Amount__c ))</errorConditionFormula>
    <errorMessage>If Does ETF Apply is Yes then ETF Amount is required.</errorMessage>
</ValidationRule>
