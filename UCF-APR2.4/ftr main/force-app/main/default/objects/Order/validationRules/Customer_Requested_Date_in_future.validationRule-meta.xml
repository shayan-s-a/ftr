<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Customer_Requested_Date_in_future</fullName>
    <active>true</active>
    <errorConditionFormula>AND(
Customer_Requested_Date__c &lt;= Today(),
ISCHANGED(Customer_Requested_Date__c)
)</errorConditionFormula>
    <errorDisplayField>Customer_Requested_Date__c</errorDisplayField>
    <errorMessage>The &quot;Customer Requested Date&quot; must be in the future.</errorMessage>
</ValidationRule>
