<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Other_Leasing_Company_Name_is_required</fullName>
    <active>true</active>
    <errorConditionFormula>AND(
ISPICKVAL(  Leased_With__c ,&quot;Other&quot;),
ISBLANK(  Other_Leasing_Company_Name__c ))</errorConditionFormula>
    <errorMessage>Other Leasing Company Name is required when Leased with is set to Other.</errorMessage>
</ValidationRule>
