<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>What_Type_of_Diversity_is_Required</fullName>
    <active>true</active>
    <description>Used on Circuit New, Change and Move Orders</description>
    <errorConditionFormula>AND(
ISPICKVAL(Is_the_Customer_Requesting_Diversity__c , &quot;Yes&quot;),
ISBLANK( What_Type_of_Diversity__c)
)</errorConditionFormula>
    <errorDisplayField>What_Type_of_Diversity__c</errorDisplayField>
    <errorMessage>What Type of Diversity is required if Is the Customer Requesting Diversity = Yes</errorMessage>
</ValidationRule>
