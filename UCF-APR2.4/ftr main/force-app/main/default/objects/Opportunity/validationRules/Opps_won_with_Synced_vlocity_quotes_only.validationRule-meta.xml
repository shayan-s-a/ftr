<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Opps_won_with_Synced_vlocity_quotes_only</fullName>
    <active>false</active>
    <errorConditionFormula>AND
(
ISBLANK(SyncedVlocityQuote__c),
IsWon,
RecordType.Name=&apos;Opportunity Lightning&apos;
)</errorConditionFormula>
    <errorMessage>Quote must be synced before Opportunity can be Closed/Won.</errorMessage>
</ValidationRule>
