<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Sales_stageClosed_WonTeam_StExecuted</fullName>
    <active>true</active>
    <errorConditionFormula>AND(RecordTypeId = &quot;012E0000000fK7P&quot;,
 ISPICKVAL(Partner_Sales_Stages__c,&quot;Closed/Won&quot;),
  NOT ISPICKVAL(Contract.Contract_Team_Status__c,&quot;Executed&quot;))</errorConditionFormula>
    <errorMessage>Cannot set the Opportunity Sales Stage to Closed/Won until the Contract Team Status is Executed</errorMessage>
</ValidationRule>
