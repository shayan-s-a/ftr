<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>zip_code_only_allows_numbers</fullName>
    <active>false</active>
    <errorConditionFormula>!REGEX(Service_Zip_code__c,&quot;[0-9]*&quot;)</errorConditionFormula>
    <errorMessage>Zip Code can only accept numbers.</errorMessage>
</ValidationRule>
