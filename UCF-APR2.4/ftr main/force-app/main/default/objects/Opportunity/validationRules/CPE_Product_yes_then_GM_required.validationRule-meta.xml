<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>CPE_Product_yes_then_GM_required</fullName>
    <active>false</active>
    <errorConditionFormula>IF(ISPICKVAL( CPE_Product__c ,&quot;yes&quot;), ISNULL( GM__c ),false)</errorConditionFormula>
    <errorDisplayField>GM__c</errorDisplayField>
