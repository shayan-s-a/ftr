<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>MDU_CRT_Submitted_Validation</fullName>
    <active>false</active>
    <description>When changing stage to CRT Submitted or some next stage, CRT Link URL field must not be null</description>
    <errorConditionFormula>AND(ISPICKVAL(StageName,&quot;CRT Submitted&quot;),
ISBLANK(CRT_Link__c), RecordType.Name = &quot;Community Connections&quot;)</errorConditionFormula>
    <errorDisplayField>CRT_Link__c</errorDisplayField>
    <errorMessage>CRT Link field can&apos;t be empty.</errorMessage>
</ValidationRule>
