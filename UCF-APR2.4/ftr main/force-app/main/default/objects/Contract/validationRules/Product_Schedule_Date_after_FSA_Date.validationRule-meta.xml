<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Product_Schedule_Date_after_FSA_Date</fullName>
    <active>true</active>
    <errorConditionFormula>AND(
RecordType.Name = &apos;Product Schedule&apos;,
StartDate &lt; FSA__r.StartDate
)</errorConditionFormula>
    <errorDisplayField>StartDate</errorDisplayField>
    <errorMessage>Product schedule start date can not start before the FSA scheduled start date</errorMessage>
</ValidationRule>
